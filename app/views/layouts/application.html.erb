<!DOCTYPE html>
<html>
  <head>
    <title>Blog</title>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
    <%= csrf_meta_tags %>
  </head>
  <body>

    <%= render 'layouts/shared/header' %>

    <div class="container">
      <div class="row">
        <%= yield %>
      </div>
    </div>

    <%= render 'layouts/shared/footer' %>

  </body>
</html>