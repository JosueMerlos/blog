<% if notice.present? %>
  <%= content_tag :div, notice, class: 'alert alert-success' %>
<% end %>
<h1>Categoria: <%= @category.id %></h1>
<table class="table">
  <tbody>
    <tr>
      <td>Nombre</td>
      <td><%= @category.nombre %></td>
    </tr>
  </tbody>
</table>

<%= link_to 'Editar', edit_category_path(@category) %> |
<%= link_to 'Atras', categories_path %>
